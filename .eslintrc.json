{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "import/first": ["error"],
    "import/order": ["error", {
      "pathGroups": [{
        "pattern": "{components,lib}/**",
        "group": "external",
        "position": "after"
      }],
      "alphabetize": {
        "order": "asc"
      },
      "warnOnUnassignedImports": true
    }]
  }
}
